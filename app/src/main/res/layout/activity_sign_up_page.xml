<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUp_Page">

    <TextView
        android:id="@+id/tvSignUp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Account"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgBgTop3" />

    <ImageView
        android:id="@+id/imgBgTop3"
        android:layout_width="451dp"
        android:layout_height="254dp"
        android:layout_marginStart="-200dp"
        android:layout_marginTop="-120dp"
        android:rotation="-45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_removal_ai__tmp_64a2d1067e48c" />

    <ImageView
        android:id="@+id/imgBgTop4"
        android:layout_width="451dp"
        android:layout_height="254dp"
        android:layout_marginTop="-145dp"
        android:layout_marginEnd="-80dp"
        android:rotation="7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_removal_ai__tmp_64a2d1067e48c" />

    <TextView
        android:id="@+id/tvSignUp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Create New "
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etUsername"
        android:layout_width="282dp"
        android:layout_height="44dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/custom_username_icon"
        android:drawablePadding="12dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="Username"
        android:inputType="text"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgBgTop3" />

    <EditText
        android:id="@+id/etEmail2"
        android:layout_width="282dp"
        android:layout_height="44dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/custom_email_icon"
        android:drawablePadding="12dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="Email Address"
        android:inputType="text|textEmailAddress"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etUsername" />

    <EditText
        android:id="@+id/etPassword2"
        android:layout_width="282dp"
        android:layout_height="46dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/custom_lock_icon"
        android:drawablePadding="12dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:hint="Password (Atleast 6 characters)"
        android:inputType="textPassword"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etEmail2" />

    <Button
        android:id="@+id/btnSignUp2"
        android:layout_width="316dp"
        android:layout_height="51dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#00BF63"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Sign Up"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etPassword2" />

    <ImageView
        android:id="@+id/imgBgBottom3"
        android:layout_width="403dp"
        android:layout_height="276dp"
        android:layout_marginEnd="-200dp"
        android:layout_marginBottom="-90dp"
        android:rotation="125"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/_removal_ai__tmp_64a2d1067e48c" />

    <ImageView
        android:id="@+id/imgBgBottom4"
        android:layout_width="459dp"
        android:layout_height="283dp"
        android:layout_marginStart="-120dp"
        android:layout_marginBottom="-150dp"
        android:rotation="-160"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/_removal_ai__tmp_64a2d1067e48c" />

    <ImageView
        android:id="@+id/imgBack2"
        android:layout_width="42dp"
        android:layout_height="37dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/imgback" />

    <TextView
        android:id="@+id/tvSignUpDesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="117dp"
        android:fontFamily="@font/poppins_light_italic"
        android:text="Already Registered ? Sign In."
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSignUp3" />


</androidx.constraintlayout.widget.ConstraintLayout>